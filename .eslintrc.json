{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true,
    "worker": true,
    "jest/globals": true,
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-use-before-define": ["error", { "functions": false }]
  }
}